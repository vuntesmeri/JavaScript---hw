//https://www.codewars.com/kata/559f860f8c0d6c7784000119/train/javascript

если преполагать что есть елементы  (damaged: false)
function anyArrows(arrows) {
  return arrows.some(
    (element) => Object.values(element).some((item) => item === true) === false
  );
}
если таких елементов быть не может 
function anyArrows(arrows) {
  return arrows.some(
    (element) => !element.damaged
  );
}

// https://www.codewars.com/kata/586909e4c66d18dd1800009b/train/javascript

function multiplyAll(arr) {
  return (factor) => arr.map((element) => element * factor);
}
